<div class="profile-section box">
	<h4>First Name:</h4>
	<p class="profile-value"><%= @profile["first_name"] %></p>
</div>
<div class="profile-section box">
	<h4>Last Name:</h4>
	<p class="profile-value"><%= @profile["last_name"] %></p>
</div>
<div class="profile-section box">
	<h4>Age:</h4>
	<p class="profile-value"><%= ((Time.now - Time.parse(@profile["dob"].to_s))/(365.25*24*60**2)).to_i %></p>
</div>
<div class="profile-section box">
	<h4>Gender:</h4>
	<p class="profile-value"><%= @profile["gender"] %></p>
</div>
